using System;

public class Solution {
    public string solution(string my_string) {
        string answer = "";
        for (int i = 0; i < my_string.Length; i++) {
            if(my_string[i] == 'a') 
                continue;
            if(my_string[i] == 'e') 
                continue;
            if(my_string[i] == 'i') 
                continue;
            if(my_string[i] == 'o') 
                continue;
            if(my_string[i] == 'u') 
                continue;
            answer = answer + my_string[i];
            }
        
        return answer;
    }
}
